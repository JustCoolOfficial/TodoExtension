{
    "manifest_version": 3,
    "name": "Justcool's todos",
    "description": "Just a private todo extension lol.",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/logo.png",
        "32": "icons/logo.png",
        "128": "icons/logo.png"
    }
}