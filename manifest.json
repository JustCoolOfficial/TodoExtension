{
    "manifest_version": 3,
    "name": "JustTodo",
    "description": "Just an easy understandable todo extension.",
    "version": "1.0.5",
    "action": {
        "default_popup": "/index.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "/assets/icons/logo.png",
        "32": "/assets/icons/logo.png",
        "128": "/assets/icons/logo.png"
    }
}