{
    "manifest_version": 3,
    "name": "JustTodos",
    "description": "Just a todos extension",
    "version": "1.0.5",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/logo.png",
        "32": "icons/logo.png",
        "128": "icons/logo.png"
    }
}